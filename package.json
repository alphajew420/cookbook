{
  "name": "cookbook-app-backend",
  "version": "1.0.0",
  "description": "Backend API for Cookbook App with AI-powered recipe and fridge scanning",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "worker": "node src/workers/scanWorker.js",
    "migrate": "node src/database/migrate.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "cookbook",
    "recipe",
    "ai",
    "gemini",
    "food",
    "api"
  ],
  "author": "Season Solutions",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "aws-sdk": "^2.1543.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^2.4.5",
    "pdf2pic": "^3.2.0",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "sharp": "^0.33.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
